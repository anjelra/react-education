{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-var": "off",
        "linebreak-style": "off",
        "prettier/prettier": "error"
    },
    "plugins": [
        "prettier"
    ]
}
